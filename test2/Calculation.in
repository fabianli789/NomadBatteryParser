##########################################################################

read 
    mol pqr A.pqr
    mol pqr B2.pqr
    mol pqr Complex.pqr
end

# CALCULATE POTENTIAL FOR FIRST COMPONENT 
elec name mol1
    mg-auto
    dime 97  129  129
    cglen 186.847 184.358  160.488
    fglen  129.910 128.446 114.405
    cgcent mol 3
    fgcent mol 3
    mol 1
    lpbe
    bcfl mdh
    ion charge 1 conc 0.01 radius 2.0
    ion charge -1 conc 0.01 radius 2.23
    pdie 4.0
    sdie 78.4
    srfm smol
    chgm spl2
    srad 1.4
    swin 0.3
    sdens 10.0
    temp 298.15
    calcenergy total
    calcforce no
end

